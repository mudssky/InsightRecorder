{
  "name": "insightrecorder",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:node-fast": "tsgo --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck:web-fast": "tsgo --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "typecheck-fast": "npm run typecheck:node-fast && npm run typecheck:web-fast",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "test": "vitest --run --config tests/vitest.config.ts",
    "test:watch": "vitest --config tests/vitest.config.ts",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "qa": "pnpm typecheck && pnpm lint:fix && pnpm format",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@mudssky/jsutils": "^1.31.0",
    "@tanstack/react-router": "^1.139.3",
    "antd": "^6.0.0",
    "better-sqlite3": "^12.4.6",
    "check-disk-space": "^3.4.0",
    "drivelist": "^12.0.2",
    "drizzle-orm": "^0.44.7",
    "electron-log": "^5.4.3",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.3.9",
    "history": "^5.3.0",
    "usb-detection": "^4.14.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript/native-preview": "7.0.0-dev.20251122.1",
    "@vitejs/plugin-react": "^5.0.3",
    "drizzle-kit": "^0.31.7",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vitest": "^4.0.13"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "drivelist",
      "electron",
      "esbuild"
    ]
  }
}
